<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.company.mapper.TestWordMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE
		FROM  test_word
		WHERE
			 sportsman_id = #{mainId}
	</delete>

	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.company.entity.TestWord">
		SELECT *
		FROM  test_word
		WHERE
			 sportsman_id = #{mainId}
	</select>
</mapper>
